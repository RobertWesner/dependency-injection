{
    "name": "robertwesner/dependency-injection",
    "description": "A fun PSR-11 container implementation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RobertWesner\\DependencyInjection\\": "src/",
            "RobertWesner\\DependencyInjection\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "RobertWesner",
            "email": "robert@wesner.io"
        }
    ],
    "require": {
        "php": ">=8.4",
        "psr/container": "^2.0",
        "vlucas/phpdotenv": "^5.6",
        "ext-simplexml": "*",
        "symfony/yaml": "^7.2",
        "softcreatr/jsonpath": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3.1",
        "squizlabs/php_codesniffer": "^3.10.2"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.xml"
    }
}
